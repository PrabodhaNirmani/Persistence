<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prabodhaharankahadeniya.persistence">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateUserActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ViewUsersActivity" />
        <activity android:name=".UpdateUserActivity" />

        <activity android:name=".FileSaveActivity"></activity>
    </application>

</manifest>